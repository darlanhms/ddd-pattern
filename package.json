{
  "private": true,
  "name": "ddd-pattern",
  "version": "1.0.0",
  "main": "bin/www",
  "license": "MIT",
  "scripts": {
    "production": "set NODE_ENV=production",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify bin/www.ts",
    "start": "set NODE_ENV=production && node dist/index.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "testWithCoverage": "jest --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "shallow-equal-object": "^1.1.1",
    "typeorm": "0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/uuid": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
